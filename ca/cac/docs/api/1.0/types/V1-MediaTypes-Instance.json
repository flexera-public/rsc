        "SoftLayer",
        "Sunrise",
        "Google",
        "Microsoft Azure",
        "HP",
        "Blue Skies",
        "VMware",
        "Datapipe",
        "IDC Frontier",
        "Logicworks"
      ],
      "type": {
        "name": "String",
        "family": "string",
        "id": "Attributor-String"
      }
    },
    "cloud_id": {
      "description": "The RightScale ID of the cloud.",
      "type": {
        "name": "Integer",
        "family": "numeric",
        "id": "Attributor-Integer"
      }
    },
    "cloud_name": {
      "description": "The name of the cloud.",
      "example_definition": "EC2 us-east-1",
      "type": {
        "name": "String",
        "family": "string",
        "id": "Attributor-String"
      }
    },
    "datacenter_key": {
      "description": "A unique identifier for the datacenter across cloud vendors.",
      "type": {
        "name": "String",
        "family": "string",
        "id": "Attributor-String"
      }
    },
