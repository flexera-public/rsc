    "deployment_id": {
      "description": "The RightScale ID of the deployment.",
      "type": {
        "name": "Integer",
        "family": "numeric",
        "id": "Attributor-Integer"
      }
    },
    "deployment_name": {
      "description": "The name of the deployment. If null the instance does not belong to a deployment.",
